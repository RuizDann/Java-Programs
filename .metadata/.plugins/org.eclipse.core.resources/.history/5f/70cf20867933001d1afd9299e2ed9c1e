
public class actMain {
	
	public static void main(String args[]) {
	
	int mySize = (int)(Math.random() * 10 + 1);

	int ary[];
	
	ary = new int[mySize];
	
	int i;
	for (i = 0; i < ary.length; i++) {
		
		int j = (int)(Math.random() * 101 - 50);
		ary[i] = j;
		System.out.println(ary[i]);
		
	}
	System.out.println(System.nanoTime() + " nanosecs");
	}
}
