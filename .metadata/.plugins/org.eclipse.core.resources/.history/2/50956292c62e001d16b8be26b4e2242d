
public class RectangleClass {
public static void main(String args[]) {
		RectangleMain r;
		
		RectangleMain ary[];
		int arySize = 5;
		//int ary[];
		ary = new RectangleMain[arySize];
		
		System.out.print("ID\t");
		
		for (int i = 0; i < ary.length; i++) {
			int l = (int)(Math.random() * 4 + 2);
			int w = (int)(Math.random() * 4 + 2);
			ary[i] = new RectangleMain(l, w);
			
			System.out.print(i+1 +"\t");
			}
			for (int j = 0; j < ary.length; j++) {
			System.out.println(ary[i].getL());
		}
			
			
		
		
	}

}
